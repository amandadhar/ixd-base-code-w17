<html>
<head>
    <title>WeHaul Demo</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="stylesheet" href="css/bootstrap.css">
    <link rel="stylesheet" href="css/bootstrap-theme.css">
    <link rel="stylesheet" href="css/customizations.css">
    <link rel="stylesheet" href="css/login.css">
</head>
<body>
<div class="container">

    <form class="form-signin">
        <div class="alert alert-danger {{status}}">
            <strong>Email already taken.</strong> Please register with a different email address.
        </div>
        <h2 class="form-signin-heading">New Account</h2>
        <label for="inputName" class="sr-only">Name</label>
        <input type="text" id="inputName" class="form-control" placeholder="Name" required autofocus>
        <label for="inputEmail" class="sr-only">Email</label>
        <input type="text" id="inputEmail" class="form-control" placeholder="Email" required>
        <label for="inputPassword" class="sr-only">Password</label>
        <input type="password" id="inputPassword" class="form-control" placeholder="Password" required>

        <a class="btn btn-lg btn-primary btn-block" id="register">Create Account</a>
        <a class="btn btn-lg btn-primary btn-block" href="/B">Return to Login</a>
    </form>
</div>

<script src="js/jquery-3.1.1.min.js"></script>
<script src="js/bootstrap.js"></script>
<script src="js/B/registerB.js"></script>
